{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "astro": "astro",
    "prebuild": "pnpm dlx npm-run-all -p check:*",
    "build": "astro build",
    "check:lint": "astro check && tsc --noEmit",
    "check:style": "pnpm exec prettier --plugin-search-dir=. --check .",
    "dev": "astro dev",
    "format": "pnpm exec prettier --plugin-search-dir=. --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm dlx npm-run-all -s build preview"
  },
  "dependencies": {
    "@astrojs/mdx": "0.19.7",
    "@astrojs/react": "2.3.2",
    "@astrojs/sitemap": "2.0.2",
    "@fontsource-variable/overpass": "5.0.9",
    "@fontsource/overpass": "5.0.8",
    "@iconify/json": "2.2.110",
    "@modyfi/vite-plugin-yaml": "1.0.4",
    "@picocss/pico": "1.5.10",
    "@svgr/core": "8.1.0",
    "@types/node": "18.17.14",
    "@types/react": "18.2.21",
    "astro": "3.0.7",
    "astro-seo": "0.8.0",
    "postcss": "8.4.29",
    "postcss-preset-env": "9.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.66.1",
    "sharp": "0.32.5",
    "typescript": "5.2.2",
    "unplugin-icons": "0.17.0",
    "vite-plugin-svgr": "3.2.0"
  },
  "packageManager": "pnpm@8.7.1"
}
