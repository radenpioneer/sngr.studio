{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "astro": "astro",
    "prebuild": "pnpm dlx npm-run-all -s check:*",
    "build": "astro build",
    "check:lint": "astro check && tsc --noEmit",
    "check:style": "pnpm exec prettier --plugin-search-dir=. --check .",
    "dev": "astro dev",
    "format": "pnpm exec prettier --plugin-search-dir=. --write .",
    "preinstall": "npx only-allow pnpm",
    "preview": "astro preview",
    "start": "pnpm dlx npm-run-all -s build preview"
  },
  "dependencies": {
    "@astrojs/mdx": "0.17.2",
    "@astrojs/react": "2.0.2",
    "@astrojs/sitemap": "1.1.0",
    "@fontsource/overpass": "4.5.11",
    "@iconify/json": "2.2.28",
    "@modyfi/vite-plugin-yaml": "1.0.4",
    "@picocss/pico": "^1.5.7",
    "@svgr/core": "6.5.1",
    "astro": "2.0.16",
    "astro-compress": "1.1.35",
    "astro-critters": "1.1.31",
    "astro-seo": "0.7.1",
    "postcss": "8.4.21",
    "postcss-preset-env": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.58.3",
    "unplugin-icons": "0.15.3"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "18.0.28",
    "typescript": "^4.9.5"
  }
