    "@types/react": "18.2.12",
    "astro": "2.6.4",
    "astro-compress": "1.1.47",
    "astro-critters": "1.1.38",
    "astro-seo": "0.7.4",
    "postcss": "8.4.24",
    "postcss-preset-env": "9.0.0",
